void show_normal_details() {
  // Check if there is data available on the serial port
  Serial.println("REQUESTING WEATHER DATA");
  while(1){
  //Serial.println("REQUESTING WEATHER DATA");
  if (Serial.available()) {
    
    // Read and print weather data
    Serial.println("REQUESTING WEATHER DATA");
    String weatherData = Serial.readStringUntil('\n');  // Read weather data until newline
    
    if (weatherData.length() > 0) {
      Serial.print("Weather Data: ");
      Serial.println(weatherData);

      // Display weather details multiple times
      for (int i = 0; i < 2; i++) {
        display_weather_details(weatherData);
      }
    } else {
      Serial.println("No weather data received.");
    }

    // Delay for clarity (optional, helps avoid immediate buffer read)
    delay(2000);

    // Request news data
    Serial.println("REQUESTING NEWS DATA");
    
    // Check if news data is available
    delay(7000);
    if (Serial.available()) {
      String newsData = Serial.readString();  // Read news data until newline
      Serial.println("gets here atleast");

      if (newsData.length() > 0) {
        Serial.print("NEWS DATA: ");
        Serial.println(newsData);

        // Display news details
        display_news_details(newsData);
        Serial.println("geths here");
        
      } else {
        Serial.println("No news data received.");
      }
    } else {
      Serial.println("No news data available.");
    }
  }

  // Add some delay to prevent continuous polling (optional)
  delay(500);  // Prevents overwhelming the loop
  break;
}}
