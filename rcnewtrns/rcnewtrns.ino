// callback when data is sent
void OnDataSent(const uint8_t *mac_addr, esp_now_send_status_t status) {
  Serial.print("\r\nLast Packet Send Status:\t");
  Serial.println(status == ESP_NOW_SEND_SUCCESS ? "Delivery Success" : "Delivery Fail");
}
void OnDataRecv(const uint8_t *mac, const uint8_t *incomingData, int len) {

  // obstacle dedected code for display with time delay
  memcpy(&obst, incomingData, sizeof(obst));
  Serial.print("Bytes received: ");
  Serial.println(len);
  Serial.print("dis: ");
  Serial.println(obst.distance);
  Serial.print("dis: ");
  Serial.println(obst.yon);
  delay(100);
  on = 0;
  display.clearDisplay();
  display.drawBitmap(0, 0, image_data_obstacle_ahead, 128, 64, 1);
  display.display();
  delay(3000);
  display.clearDisplay();
  on = 1;







  // ledcWrite(pwmChannel, myData.acc);
}

void setup() {
  // Init Serial Monitor
  Serial.begin(115200);
  pinMode(2, INPUT);//turbo_button
  pinMode(18,INPUT);
  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;)
      ;
  }
  display.clearDisplay();
  delay(2000);  // Pause for 2 seconds

  // Set device as a Wi-Fi Station
  WiFi.mode(WIFI_STA);

  // Init ESP-NOW
  if (esp_now_init() != ESP_OK) {
    Serial.println("Error initializing ESP-NOW");
    return;
  }

  // Once ESPNow is successfully Init, we will register for Send CB to
  // get the status of Trasnmitted packet
  esp_now_register_recv_cb(OnDataRecv);
  esp_now_register_send_cb(OnDataSent);

  // Register peer
  memcpy(peerInfo.peer_addr, broadcastAddress, 6);
  peerInfo.channel = 0;
  peerInfo.encrypt = false;

  // Add peer
  if (esp_now_add_peer(&peerInfo) != ESP_OK) {
    Serial.println("Failed to add peer");
    return;
  }
}
void loop() {
  // Set values to send
  turbo_mode=digitalRead(2);
  ultramode=digitalRead(18);
  int x = analogRead(pot1);
  int y = analogRead(pot2);
  float dut_acc = x / 16.058;
  float dut_turn = y / 16.058;
  myData.acc = (int)dut_acc;
  myData.turn = y;
  myData.ultmode = ultramode;
  myData.turbmode = turbo_mode;
  Serial.println((int)dut_acc);
  Serial.println((int)dut_turn);
  Serial.println(turbo_mode);
  Serial.println(ultramode);
  
  esp_err_t result = esp_now_send(broadcastAddress, (uint8_t *)&myData, sizeof(myData));
  if (result == ESP_OK) {
    Serial.println("Sent with success");
  } else {
    Serial.println("Error sending the data");
  }

  // Draw bitmap on the screen

  if (y >= 3200) {
    dir = 1;
  } else if (y <= 2200) {
    dir = 2;
  } else {
    dir = 0;
  }
  if ((turbo_mode == 1) && (on == 1)) {

    if ((myData.acc >= 160) && (myData.acc <= 175) && (dir == 0)) {  //===============================
      Serial.print("Gets Here");
      display.drawBitmap(0, 0, image_data_turbo_straight, 128, 64, 1);
      display.setFont(&FreeMonoBold9pt7b);
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(30, 23);
      // Display static text
      display.println(0);
      display.display();
      delay(50);
      //display.clearDisplay();
    } else if ((myData.acc >= 170) && (dir == 0)) {  //STRAIGHT TURBO
      dut = (int)(myData.acc - 170) * 3 / 2.55;      //remove 2.55 to get dutycycle
      display.drawBitmap(0, 0, image_data_turbo_straight, 128, 64, 1);
      display.setFont(&FreeMonoBold9pt7b);
      display.setTextSize(1);
      display.setTextColor(WHITE);
      display.setCursor(30, 23);
      display.println(dut);
      display.display();
      delay(50);
      //display.clearDisplay();
      delay(1);
    }

    else if ((myData.acc <= 160) && (dir == 0)) {  //=======straight========================
