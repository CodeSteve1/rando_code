void display_news_details(String data) {
  // Step 2: Split the raw data using "', " as the delimiter
    data ="'@@Chennai Petroleum in Talks to Raise $3.3 Billion for Refinery@@', '@@China urges India to handle Taiwan issue cautiously after Mumbai office opening@@', '@@Rekha at 70: Bollywood's timeless icon@@', '@@Why hundreds of Samsung workers are protesting in India@@', '@@Drone video shows flooding in Indias Chennai@@', '@@Ashwin shines as India thrash Bangladesh in first Test@@'";

  String rawData = data;
  rawData.replace("@@", "");
  int startIndex = 0, endIndex = 0;

  String newsItems[6];  // Declare an array to hold news items
  int newsCount = 0;    // Initialize a count for the news items

  while (newsCount < 6) {  // Ensure we don't exceed the array bounds
    endIndex = rawData.indexOf("', ", startIndex);
    if (endIndex == -1) {  // Break if no more items are found
      break;
    }
    newsItems[newsCount] = rawData.substring(startIndex, endIndex);
    startIndex = endIndex + 3;  // Move past the delimiter
    newsCount++;
  }
  // Handle the last item
  if (startIndex < rawData.length()) {
    newsItems[newsCount] = rawData.substring(startIndex);
    newsCount++;
  }

  // Print the results
  Serial.println("Parsed News Items:");
  for (int i = 0; i < newsCount; i++) {
    Serial.println("News " + String(i + 1) + ": " + newsItems[i]);
  }

  // Display the news items
  for (int i = 0; i < newsCount; i++) {
    display.clearDisplay();  // Clear display for each new item
    display.setCursor(10, 11);
    display.setTextSize(1);
    display.setTextColor(WHITE);
    display.println(newsItems[i]);
    display.display();
    delay(4000);  // Wait before showing the next item
  }
}




