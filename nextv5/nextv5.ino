void send_message() {
  while (1) {
    char key = keypad.getKey();
    if (LoRa.parsePacket()) {
      Serial.println("Some data available");
      String LoRaData = "";
      while (LoRa.available()) {
        char c = (char)LoRa.read();
        if (c == '\n') {  // Check for the termination character
            break;  // Exit loop if end of message is detected
        }
        LoRaData += c;  // Append character to LoRaData
    }
      if (LoRaData.length() > 0) {
        Serial.print("Received: ");
        Serial.println(LoRaData);
        upd_disp_recv(LoRaData);  // Update OLED display with received data
      }
    }
    keypad.getKey();
    if (key) {
      int keyIndex = -1;

      if (key == '*') {  // Handle backspace
        if (typedText.length() > 0) {
          typedText.remove(typedText.length() - 1);
          Serial.print("Backspace pressed. Current text: ");
          Serial.println(typedText);
          updateDisplay();  // Update the OLED display after backspace
        }
        continue;  // Keep looping
      }

      else if (key >= '1' && key <= '9') {
        keyIndex = key - '1';
      } else if (key == '0') {
        keyIndex = 9;
      } else if (key == '#') {
        LoRa.print(typedText + "\n");  // Append a newline character to mark the end of the message
        LoRa.endPacket();
        Serial.println("sent via lora");
        typedText = "";
      }
      else if(key == 'B'){
        show_normal_details() ;
      }
      else if(key == 'C'){
        LoRa.setSyncWord(0xFF);

        String lat="12.869220";
        String lon="80.219379";
        LoRa.println(lat+"@@"+lon);
        LoRa.endPacket();
        Serial.println("SOS");
        LoRa.setSyncWord(0xF3);
      }


      if (keyIndex != -1) {
        if (lastKey == keyIndex && (millis() - lastPressTime) < pressTimeout) {
          pressCount++;                            // Move to the next letter
          pressCount %= strlen(keyMap[keyIndex]);  // Cycle through the letters
        } else {
          if (lastKey != -1) {
            char finalChar = keyMap[lastKey][pressCount];
            typedText += finalChar;  // Append the selected letter to typedText
            updateDisplay();         // Update the OLED display with new text
          }
          pressCount = 0;
        }

        lastKey = keyIndex;
        lastPressTime = millis();

        char currentChar = keyMap[keyIndex][pressCount];  // Current character being cycled
        Serial.print("Cycling Character: ");
        Serial.println(currentChar);
        updateDisplay();  // Display current character
      }
    }

    if (lastKey != -1 && (millis() - lastPressTime) > pressTimeout) {
      char finalChar = keyMap[lastKey][pressCount];
      typedText += finalChar;  // Append the final letter to typedText
      Serial.print("Character automatically selected after timeout: ");
      Serial.println(finalChar);

      lastKey = -1;     // Reset key tracking
      updateDisplay();  // Update the OLED display with the finalized text
    }

    updateDisplay();
  }
}
void upd_disp_recv(String stri) {
  display.clearDisplay();
  display.setCursor(10, 11);
  display.setTextSize(1);
  display.setFont();
  display.setTextColor(WHITE);
  display.println(stri);
  display.display();
  while(1){
    char key=keypad.getKey();
    if (key=='#'){
      break;
    }
  }
}
